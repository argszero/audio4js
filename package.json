  "license": "MIT",
  "devDependencies": {
    "cargo-cp-artifact": "^0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argszero/audio4js.git"
  },
  "keywords": [
    "audio",
    "player",
    "mp3",
    "rodio",
    "playback"
  ],
  "bugs": {
    "url": "https://github.com/argszero/audio4js/issues"
  },
  "homepage": "https://github.com/argszero/audio4js#readme"
}
